{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/alimentacion/**",
        "destination": "/alimentacion/index.html"
      },
      {
        "source": "/combustibles/**",
        "destination": "/combustibles/index.html"
      }
    ]
  }
}